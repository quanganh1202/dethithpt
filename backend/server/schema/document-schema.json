{
  "$id": "http://dethithpt.com/document-schema#",
  "title": "Document",
  "description": "Document schema validation",
  "type": "object",
  "additionalProperties": false,
  "required":[ "name", "tags", "userId", "price", "cateId", "subjectId", "classId", "yearSchoolId", "collectionId" ],
  "properties": {
    "name": { "type": "string", "maxLength": 255 },
    "tags": {
      "type": "string"
    },
    "description": { "type": "string" },
    "userId": { "type": "string" },
    "price": { "type": "string", "maxLength": 50, "pattern": "^[0-9]+$"},
    "cateId": { "type": "string", "maxLength": 11 },
    "subjectId": { "type": "string", "maxLength": 11 },
    "classId": { "type": "string", "maxLength": 11 },
    "yearSchoolId": { "type": "string", "maxLength": 4 },
    "collectionId": { "type": "string", "maxLength": 11 }
  }
}