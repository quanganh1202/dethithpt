{
  "$id": "http://dethithpt.com/category-update-schema#",
  "title": "Category",
  "description": "Category schema validation",
  "type": "object",
  "additionalProperties": false,
  "required": ["userId"],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "description": {
      "type": "string",
      "minLength": 1
    },
    "userId": {
      "type": "string",
      "minLength": 1
    },
    "priority": {
      "type": "integer",
      "enum": [0, 1]
    },
    "position": {
      "type": "string",
      "minLength": 1,
      "pattern": "/^[0-9]+$/"
    }
  }
}